{
  "name": "plataforma-zaid",
  "version": "0.1.0",
  "private": true,
  "description": "Plataforma de Multiatendimento WhatsApp",
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "dev:api": "pnpm --filter @zaid/api dev",
    "dev:web": "pnpm --filter @zaid/web dev",
    "build": "pnpm run --parallel build",
    "build:api": "pnpm --filter @zaid/api build",
    "build:web": "pnpm --filter @zaid/web build",
    "start": "pnpm run --parallel start",
    "lint": "pnpm run --parallel lint",
    "db:generate": "pnpm --filter @zaid/api prisma:generate",
    "db:migrate": "pnpm --filter @zaid/api prisma:migrate",
    "db:studio": "pnpm --filter @zaid/api prisma:studio",
    "db:seed": "pnpm --filter @zaid/api prisma:seed",
    "clean": "rimraf node_modules apps/*/node_modules packages/*/node_modules"
  },
  "keywords": [
    "whatsapp",
    "multiatendimento",
    "chat",
    "nestjs",
    "nextjs"
  ],
  "author": "",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
